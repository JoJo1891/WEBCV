<form method="post" action="{{ path('contact_information_delete', {'id': contact_information.id, 'idcv': cv}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ contact_information.id) }}">
    <div class="row right">
    	<button class="red waves-effect waves-light btn">Delete</button>
    </div>
</form>
